<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.rememorydroid.oriolsecall.rememorydroid.RespirarActivity">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/ivRespirar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical|center_horizontal|center"
            android:cropToPadding="true"
            android:foregroundGravity="center_vertical|fill_vertical|center_horizontal|fill_horizontal|center|fill"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/abstracte1" />
    </FrameLayout>
</RelativeLayout>
